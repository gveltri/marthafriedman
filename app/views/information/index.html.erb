<div class="site-wrapper-inner">
  <div class="cover-container">
    <div class="inner">
<p id="notice"><%= notice %></p>

<h1>Listing Information</h1>
<div class="table-responsive" style="overflow-y: scroll; height: 300px;">
<table class="table" >
  <thead>
	  <tr>
		  <th>Version</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @information.reverse_each do |information| %>
	    <tr>
		    <td><%= information.version %></td>
        <td><%= link_to 'Show', information %></td>
        <td><%= link_to 'Edit', edit_information_path(information) %></td>
        <td><%= link_to 'Destroy', information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

<br>

<%= link_to 'New Information', new_information_path %>


    </div>
  </div>
</div>
